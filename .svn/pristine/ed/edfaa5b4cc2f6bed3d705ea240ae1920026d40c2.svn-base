package br.gov.ce.saude.ponto.vo;

import br.gov.ce.saude.ponto.entidade.EscalaItem;
import java.io.Serializable;
import java.math.BigInteger;
import java.util.List;

/**
 *
 * @author esmayktillesse
 */
public class FuncionarioHorarioVO implements Serializable {
    
    private static final long serialVersionUID = -2038093105748201345L;
    
    private BigInteger id;
    private List<EscalaItem> dias;

    public FuncionarioHorarioVO() {
    }

    public FuncionarioHorarioVO(BigInteger id, List<EscalaItem> dias) {
        this.id = id;
        this.dias = dias;
    }

    public BigInteger getId() {
        return id;
    }

    public void setId(BigInteger id) {
        this.id = id;
    }

    public List<EscalaItem> getDias() {
        return dias;
    }

    public void setDias(List<EscalaItem> dias) {
        this.dias = dias;
    }
    
    
}
